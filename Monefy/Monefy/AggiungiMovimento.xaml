        <Image Margin="418,363,57,-2" Source="incassi.jpg" Stretch="Fill"/>
        <Image Margin="0,0,0,367" Source="logo2.png" Stretch="Fill"/>
        <TextBox x:Name="nota" HorizontalAlignment="Left" Height="32" TextWrapping="Wrap" VerticalAlignment="Top" Width="284" Margin="74,150,0,0" FontSize="20" Background="{x:Null}">
            <TextBox.BorderBrush>
                <SolidColorBrush Color="#FFABADB3" Opacity="0"/>
            </TextBox.BorderBrush>
        </TextBox>
        <Label x:Name="recap" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="12,311,0,0" Height="39" Width="346" FontSize="20" Foreground="#FF853434"/>
